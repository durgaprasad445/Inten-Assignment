n = int(input('Enter any value: '))
m = input('Please Enter Your Name: ')
for i in range(1,n): #loop
  if i%3 and i%5 == 0: #checks if 'i' is a multiple of both 3 & 5
    print(m)
  elif i%3 ==0:  #checks if 'i' is a multiple of 3
    print('3n')
   elif i%5 == 0: #checks if 'i' is a multiple of 5
    print('5n')  
   else:
    print(False)
